#include<stdio.h>
int main()
{
int c;
printf("enter your choice\n 1.Pizza, Rs 239\n 2.Burger, Rs 129\n 3.Pasta, Rs 179\n 4.French Fries, Rs 99\n 5.Sandwich, Rs 149\n");
scanf("%d",&c);
switch(c)
{
case 1:
printf("pizza Rs 239\n");
break;

case 2:
printf("Burger Rs 129\n");
break;

case 3:
printf("pasta Rs 179\n");
break;

case 4:
printf("French Fries Rs 99\n");
break;

case 5:
printf("Sandwich Rs 149\n");
break;

default:
printf("please select from the menu");
break;
}
return 0;
}
